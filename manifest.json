{
    "manifest_version": 2,

    "name": "New Tab",
    "description": "This tab is for customize new tab default content",
    "version": "1.0",

    "browser_action": {
        "default_icon": "64x64.png"
            //"default_popup": "popup.html"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "content_scripts": [{
        "matches": ["http://*/*", "https://*/*"],
        "css": ["style.css"],
        "js": ["jquery-3.0.0.min.js", "popup.js"]
    }],
    "icons": {
        "16": "16x16.png",
        "32": "32x32.png",
        "48": "48x48.png",
        "64": "64x64.png"
    },
    "chrome_url_overrides": {
        "newtab": "tabContent.html"
    },
    "web_accessible_resources": [
        "toolbar.html",
        "style.css"
    ],
    "incognito": "split"
}
